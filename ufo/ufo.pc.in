prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=@CMAKE_INSTALL_PREFIX@
libdir=${exec_prefix}/@CMAKE_INSTALL_LIBDIR@
includedir=${prefix}/@CMAKE_INSTALL_INCLUDEDIR@
girdir=${prefix}/@CMAKE_INSTALL_GIRDIR@
typelibdir=${exec_prefix}/@CMAKE_INSTALL_TYPELIBDIR@
plugindir=${exec_prefix}/@CMAKE_INSTALL_PLUGINDIR@
kerneldir=${prefix}/@CMAKE_INSTALL_KERNELDIR@

Name: @UFO_NAME@
Description: @UFO_DESCRIPTION_SUMMARY@
Version: @UFO_VERSION@
Libs: -L${libdir} -lufo
Cflags: -I${includedir}
Requires: glib-2.0 gobject-2.0 gmodule-2.0 gio-2.0
